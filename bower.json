{
  "name": "spa-skeleton",
  "version": "0.0.1",
  "main": "public/javascripts/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "backbone": "~1.1.0",
    "underscore": "~1.5.2",
    "bootstrap": "~3.1.0",
    "console-polyfill": "~0.1.0",
    "jquery": "~1.11.0",
    "moment": "~2.6.0",
    "bootstrap-switch": "*",
    "i18next": "~1.7.1",
    "Faker": "~0.5.11",
    "backbone-validation": "~0.9.1",
    "backbone.marionette": "~1.6.1",
    "font-awesome": "~4.0.3",
    "bluebird": "~1.0.4",
    "loggerz": "~0.0.1",
    "bootstrap-datepicker": "~1.3.0",
    "select2": "~3.4.5",
    "numeral": "~1.5.3",
    "bootstrap-multiselect": "~0.9.4",
    "moment-timezone": "~0.0.6",
    "d3": "~3.4.8"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "underscore": "*",
        "jquery": "*"
      },
      "main": "backbone.js"
    },
    "bootstrap": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "dist/fonts/*"
      ]
    },
    "backbone.marionette": {
      "dependencies": {
        "backbone": "*"
      }
    },
    "backbone-validation": {
      "dependencies": {
        "backbone": "*"
      },
      "main": "src/backbone-validation.js"
    },
    "bootstrap-switch": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "static/js/bootstrap-switch.js",
        "static/stylesheets/bootstrap-switch.css"
      ]
    },
    "underscore": {
      "main": "underscore.js"
    },
    "i18next": {
      "main": "i18next.js"
    },
    "Faker": {
      "main": "Faker.js"
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/*"
      ]
    },
    "bluebird": {
      "main": "js/browser/bluebird.js"
    },
    "jquery": {
      "main": "dist/jquery.js"
    },
    "loggerz": {
      "main": "main/browser/logger.js"
    },
    "bootstrap-datepicker": {
      "main": [
        "js/bootstrap-datepicker.js",
        "js/locales/bootstrap-datepicker.fr.js"
      ]
    },
    "select2": {
      "main": [
        "select2.js",
        "select2.css",
        "select2-bootstrap.css",
        "select2-spinner.gif",
        "select2.png"
      ]
    },
    "bootstrap-multiselect": {
      "main": [
        "js/bootstrap-multiselect.js",
        "css/bootstrap-multiselect.css"
      ]
    },
    "moment": {
      "main": [
        "min/moment-with-langs.js"
      ]
    },
    "moment-timezone": {
        "dependencies": {
            "moment": "*"
        },
    "main": [
      "moment-timezone.js"
    ]
  },
    "numeral": {
      "main": [
        "numeral.js",
        "languages.js"
      ]
    }
  }
}
